<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="323.0" prefWidth="553.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quizapp.ui.SigninController">
    <children>
        <VBox alignment="TOP_CENTER" layoutY="56.0" prefHeight="427.0" prefWidth="553.0" spacing="10.0">
            <children>
                <Label text="Sign in">
                    <font>
                        <Font size="61.0" />
                    </font>
                </Label>
                <Label text="Username:">
                    <VBox.margin>
                        <Insets top="40.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="username" maxWidth="200.0" minWidth="100.0" prefWidth="100.0" />
                <Label text="Password:">
                    <VBox.margin>
                        <Insets top="15.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="password" maxWidth="200.0" minWidth="200.0" prefWidth="200.0" />
                <Button mnemonicParsing="false" onAction="#toMainMenu" text="Sign in">
                    <VBox.margin>
                        <Insets top="40.0" />
                    </VBox.margin>
                </Button>
                <Label fx:id="errorMessage" />
            </children>
        </VBox>
    </children>
</AnchorPane>
